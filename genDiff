import _ from "lodash";

const file1 = {
    "host": "hexlet.io",
    "timeout": 50,
    "proxy": "123.234.53.22",
    "follow": false
};

const file2 = {
    "timeout": 20,
    "verbose": true,
    "host": "hexlet.io"
};

function gendiff (file1, file2) {
  const sortedKeys = _.SortBy(Object.keys({ ...file1, ...file2 }));
  console.log(sortedKeys);
  return sortedKeys;
}
// С помощью массива ключей я проверяю есть ли объекты в 1 и во 2 объекте и формирую массив с данными по ключам
// Используя массив с данными по ключам я формирую строку

export {
    gendiff,
}